[{
  "name": "api-dev",
  "image": "rorcommunity/ror-api:dev",
  "cpu": 512,
  "memory": 2048,
  "networkMode": "awsvpc",
  "essential": true,
  "portMappings": [{
    "containerPort": 80,
    "hostPort": 80
  }],
  "logConfiguration": {
    "logDriver": "awslogs",
    "options": {
      "awslogs-group": "/ecs/api-dev",
      "awslogs-region": "eu-west-1",
      "awslogs-stream-prefix": "ecs"
    }
  },
  "environment": [{
      "name": "PASSENGER_APP_ENV",
      "value": "production"
    },
    {
      "name": "RELEASE_STAGE",
      "value": "dev"
    },
    {
      "name": "ELASTIC7_HOST",
      "value": "${elastic7_host_dev}"
    },
    {
      "name": "ELASTIC7_PORT",
      "value": "${elastic7_port_dev}"
    },
    {
      "name": "AWS_ACCESS_KEY_ID",
      "value": "${access_key}"
    },
    {
      "name": "AWS_SECRET_ACCESS_KEY",
      "value": "${secret_key}"
    },
    {
      "name": "AWS_REGION",
      "value": "${region}"
    },
    {
      "name": "SECRET_KEY",
      "value": "${django_secret_key}"
    },
    {
      "name": "PUBLIC_KEY",
      "value": "${public_key}"
    },
    {
      "name": "SENTRY_DSN",
      "value": "${sentry_dsn}"
    },
    {
      "name": "GITHUB_VERSION",
      "value": "${version}"
    },
    {
      "name": "TOKEN",
      "value": "${token}"
    },
    {
      "name": "DATA_STORE",
      "value": "${data_store}"
    },
    {
        "name": "PUBLIC_STORE",
        "value": "${public_store}"
    },
    {
      "name": "ROUTE_USER",
      "value": "${route_user}"
    },
    {
      "name": "GITHUB_TOKEN",
      "value": "${github_token}"
    },
    {
      "name": "LAUNCH_DARKLY_KEY",
      "value": "${launch_darkly_key}"
    },
    {
      "name": "DB_HOST",
      "value": "${db_host}"
    },
    {
      "name": "DB_PORT",
      "value": "${db_port}"
    },
    {
      "name": "DB_NAME",
      "value": "${db_name}"
    },
    {
      "name": "DB_USER",
      "value": "${db_user}"
    },
    {
      "name": "DB_PASSWORD",
      "value": "${db_password}"
    }
  ]
}]
